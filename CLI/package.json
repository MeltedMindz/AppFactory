{
  "name": "appfactory",
  "version": "1.0.0",
  "description": "CLI for the App Factory pipeline - Generate store-ready Expo React Native apps using Claude AI",
  "main": "dist/src/index.js",
  "bin": {
    "appfactory": "./dist/src/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "start": "node dist/src/index.js",
    "smoke": "npm run build && node dist/tests/smoke.js",
    "test": "npm run smoke",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublish": "npm run build"
  },
  "keywords": [
    "cli",
    "expo",
    "react-native",
    "ai",
    "claude",
    "anthropic",
    "app-generator",
    "mobile-app"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "ajv": "^8.17.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "inquirer": "^12.2.0",
    "ora": "^8.1.0"
  },
  "devDependencies": {
    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.10.0",
    "typescript": "^5.6.3"
  }
}
